#include<stdio.h>
int main(){
    int n;
    printf("Enter Octal number:");
    scanf("%o",&n);
    printf("Decimal of %o is %i",n,n);
    return 0;
}



                    OR

#include<stdio.h>
int main(){
    int num;
    printf("Enter Octal Number:");
    scanf("%i",&num);
    int temp=num,decnum=0,base=1;
    while(temp != 0){
        int rem=temp%10;
        decnum+=rem*base;
        base*=8;
        temp=temp/10;
    }
    printf("The decimal of %i is %i",num,decnum);
    return 0;
}
