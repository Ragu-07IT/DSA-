  #include<stdio.h>

  int convertOctaltoDecimal(int octalnum){
    int dec=0,base=1;
    while(octalnum!=0){
        int rem=octalnum%10;
        dec+=rem*base;
        base*=8;
        octalnum/=10;
    }
    return dec;
  }


  int convertDecimaltoBinary(int decnum){
    int bin=0,place=1;
    while(decnum!=0){
        int rem=decnum%2;
        bin+=rem*place;
        place*=10;
        decnum/=2;
    }
    return bin;
  }

  int main(){
    int num;
    printf("Enter Octal Number:");
    scanf("%d",&num);
    int binary=convertDecimaltoBinary(convertOctaltoDecimal(num));
    printf("The Binary of %i is %i",num,binary);
    return 0;
  }
